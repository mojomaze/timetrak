$("#list_value_<%= @list_value.id %>").replaceWith("<%= escape_javascript(render(@list_value)) %>");

$("#temp_errors").attr('id', 'errors');

$('input').each(function(i) {
  var tmp = $(this).attr('id').substring(5);
	$(this).attr('id', tmp);
});
$("label[for]").each(function(i) {
  var tmp = $(this).attr('for').substring(5);
	$(this).attr('for', tmp);
});

$("#show-add-item").show();
$(".edit").show();
$(".delete").show();
